<% extend('../layout/master') %>
    <div ng-controller="account">
        <form>
            <input type="text" placeholder="username" ng-model="account.username">
            <input type="text" placeholder="password" ng-model="account.password">
            <input type="number" placeholder="phone" ng-model="account.phone">
            <input type="text" placeholder="address" ng-model="account.address">
            <input type="text" placeholder="email" ng-model="account.email">
            <select ng-model="account.level">
                <option value="">Admin Store</option>
                <option value="1">Admin System</option>
                <option value="2">Admin Sale</option>
            </select>
            <!-- <input type="text" placeholder="nameStore" ng-model="store.name"> -->
            <button ng-click="createAccount()">Create</button>
        </form>
    </div>